{
  "Css Module": {
    "prefix": "cm",
    "description": "CssModuleDefault",
    "body": [".block_container {", "}"]
  },
  "Vue Tsx Module": {
    "prefix": "vtm",
    "description": "Vue Tsx Module",
    "body": [
      "import { defineComponent } from 'vue'",
      "import style from './index.module.less'",
      "export default defineComponent ({",
      "  setup() {",
      "    return () => (",
      "      <div class={style['block__container']}>",
      "      </div>",
      "    );",
      "  },",
      "});"
    ]
  },
  "Nuxt Tsx Default": {
    "prefix": "ntd",
    "description": "Nuxt Tsx Default",
    "body": [
      "<script setup lang='tsx'>",
      "interface Data {",
      "val: any",
      "}",
      "",
      "interface Props {",
      "value?: any",
      "}",
      "",
      "interface Emits {",
      "  (event: 'change', _para: any): void",
      "}",
      "",
      "const data = reactive<Data>({",
      "  val: ''",
      "})",
      "",
      "const props = withDefaults(",
      "  defineProps<Props>(),",
      "  {",
      "    value: 'value'",
      "  }",
      ")",
      "",
      "const emits = defineEmits<Emits>()",
      "",
      "const changeFun = () => {",
      "  emits('change', {})",
      "}",
      "",
      "const blockCls = useNamespace('block')",
      "",
      "defineRender(() => {",
      "  return (",
      "   <div ",
      "    class={blockCls.b('container')}",
      "    onClick={(_: MouseEvent) => changeFun()}",
      "   >",
      "    {",
      "     data.val",
      "    }",
      "    {",
      "      props.value",
      "    }",
      "   </div>",
      "  )",
      "})",
      "</script>",
      "",
      "<style lang='scss' scoped>",
      ".J-block-container {",
      "}",
      "</style>"
    ]
  },
  "Vue Tsx Setup": {
    "prefix": "vts",
    "description": "Vue Tsx Setup",
    "body": [
      "<script lang='tsx' setup>",
      "defineRender(() => {",
      "  return (",
      "    <div class={'block__container'}>",
      "    </div>",
      "  )",
      "})",
      "</script>",
      "",
      "<style lang='scss' scoped>",
      ".block__container {",
      "}",
      "</style>"
    ]
  },
  "Uni Vue Setup": {
    "prefix": 'uvs',
    "description": "Uni Vue Setup",
    "body": [
      "<script lang='ts' setup>",
      "import { useNamespace } from '@/hooks/useNamespace'",
      "const cls = useNamespace('page')",
      "</script>",
      "",
      "<template>",
      "  <div :class="cls.e('container')>",
      "  </div>",
      "</template>",
      "",
      "<style lang='scss' scoped>",
      "$cls: page;",
      ".#{$cls} {",
      "   &__container {",
      "   }",
      "}",
      "</style>"
    ]
  }
}
